{
    "name": "becagis/laravel-geoserver",
    "description": "This is my package laravel-geoserver",
    "keywords": [
        "becagis",
        "laravel",
        "laravel-geoserver"
    ],
    "homepage": "https://github.com/becagis/laravel-geoserver",
    "license": "MIT",
    "authors": [
        {
            "name": "becagis",
            "email": "becagis.vntts@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.13.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelGeoserver\\LaravelGeoserver\\": "src",
            "LaravelGeoserver\\LaravelGeoserver\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelGeoserver\\LaravelGeoserver\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelGeoserver\\LaravelGeoserver\\LaravelGeoserverServiceProvider"
            ],
            "aliases": {
                "LaravelGeoserver": "LaravelGeoserver\\LaravelGeoserver\\Facades\\LaravelGeoserver"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}